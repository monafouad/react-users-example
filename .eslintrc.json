{
  "root": true,
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "parser": "babel-eslint",

  "overrides": [],

  "plugins": ["react"],
  "rules": {
    "react/prop-types": "off",
    "react/no-unescaped-entities": 0
  }
}
